CA,QAAA,QAAA,CAAA,MAAA;AACA,OAFA;AAGA;AA7CA;AAFA,CAAA","sourcesContent":["<template>  \n      <div class=\"echart\" id=\"echart-line\" :style=\"{float:'left',width: '100%', height: '230px'}\"></div>   \n</template>\n\n<script>\n\nimport echarts from  \"echarts\";\n\nexport default {\n     \n      methods:{\n\n      initChart(name,xData,yData) {\n       \n          let getchart = echarts.init(document.getElementById('echart-line'));\n          var  option = {\n               \n                tooltip: {\n                    trigger: 'axis'\n                },\n                legend: {\n                    data: [name],\n                    bottom:'0%'\n                },\n                grid: { //调整图表上下左右位置\n                    top:'10%',\n                    left: '3%',\n                    right: '8%',\n                    bottom: '11%',\n                    containLabel: true\n                },\n               \n                xAxis: {\n                    type: 'category',\n                    boundaryGap: false,\n                    data: xData\n                },\n                yAxis: {\n                    type: 'value'\n                },\n                series: [\n                    {\n                        name: name,\n                        type: 'line',\n                        stack: '总量',\n                        data: yData\n                    },\n                ]\n            };\n\n          getchart.setOption(option);\n          //随着屏幕大小调节图表\n          window.addEventListener(\"resize\", () => {\n            getchart.resize();\n        });\n      },\n\n    }\n\n}\n</script>\n"],"sourceRoot":"src/components/basic"}]}